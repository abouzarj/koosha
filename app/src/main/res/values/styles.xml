<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.BottomNavigationView"
        parent="Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/estedad</item>
    </style>

    <style name="ebi" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
        <item name="android:color">@color/white</item>
    </style>

    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">

        <item name="android:layoutDirection">rtl</item>
        <item name="android:textAlignment">gravity</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:state_focused">true</item>
        <item name="fontFamily">@font/estedad</item>

    </style>

    <style name="ShapeAppearanceOverlayExtended" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="Final" parent="">
        <item name="fontFamily"> @font/estedad </item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginBottom">4dp</item>
    </style>

    <style name="MyDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@color/white</item>
        <item name="android:gravity">right</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="AlertDialogMessageFontStyle" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:fontFamily">@font/estedad</item> <!-- Replace "your_font_file" with the name of your font file -->
    </style>



    <style name="cardviewprofile">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:elevation">1dp</item>
        <item name="strokeColor">@color/gr_300</item>
        <item name="strokeWidth">1dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>


</resources>